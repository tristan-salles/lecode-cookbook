<?xml version="1.0" encoding="UTF-8"?>
<lecode:lecodeInput xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:lecode="http://subversion.assembla.com/svn/xsd/XSDlecode"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xsi:schemaLocation="http://subversion.assembla.com/svn/xsd/XSDlecode
	http://subversion.assembla.com/svn/xsd/XSDlecode/LECODE.xsd">

	<struct-Mesh-TIN>
		<Strata_grid>
			<StrataGrid>data/layers.nodes</StrataGrid>
			<GridX>161</GridX>
			<GridY>161</GridY>
			<GridSpace>1875</GridSpace>
			<GridXo>0.0</GridXo>
			<GridYo>0.0</GridYo>
		</Strata_grid>

		<TIN_refine>
			<HighRes>1875</HighRes>
			<LowRes>3750</LowRes>
			<StepFill>0.001</StepFill>
			<FlowAcc>20</FlowAcc>
		</TIN_refine>

		<Boundary>
			<North>0</North>
			<South>0</South>
			<East>0</East>
			<West>0</West>
		</Boundary>
	</struct-Mesh-TIN>

	<struct-time>
		<Time>
			<startTime>50000</startTime>
			<endTime>200000</endTime>
			<layerTime>5000</layerTime>
			<outputTime>25000</outputTime>
			<riverInterval>500</riverInterval>
			<rainInterval>250</rainInterval>
			<SlopeInterval>1000</SlopeInterval>
			<samplingInterval>1000</samplingInterval>
		</Time>

		<!--CheckPointing>
			<Frequency>5</Frequency>
		</CheckPointing-->
	</struct-time>

	<struct-ocean>
		<SeaLevelFluctuations>
			<SeaLevelFile>data/sea.sl</SeaLevelFile>
		</SeaLevelFluctuations>

		<WaterDensities>
			<enteringFluid>1.015E3</enteringFluid>
			<seaDensity>1.027E3</seaDensity>
		</WaterDensities>
	</struct-ocean>

	<struct-geodyn>
		<Displacement>
			<nbDispInterval>6</nbDispInterval>
			<disp>
				<startDispTime>55740</startDispTime>
				<endDispTime>55741</endDispTime>
				<dispFile>data/disp1.disp</dispFile>
			</disp>
			<disp>
				<startDispTime>167220</startDispTime>
				<endDispTime>167221</endDispTime>
				<dispFile>data/disp2.disp</dispFile>
			</disp>
			<disp>
				<startDispTime>222959</startDispTime>
				<endDispTime>222960</endDispTime>
				<dispFile>data/disp3.disp</dispFile>
			</disp>
			<disp>
				<startDispTime>278699</startDispTime>
				<endDispTime>278700</endDispTime>
				<dispFile>data/disp4.disp</dispFile>
			</disp>
			<disp>
				<startDispTime>377419</startDispTime>
				<endDispTime>377420</endDispTime>
				<dispFile>data/disp5.disp</dispFile>
			</disp>
			<disp>
				<startDispTime>463860</startDispTime>
				<endDispTime>463861</endDispTime>
				<dispFile>data/disp6.disp</dispFile>
			</disp>
		</Displacement>
	</struct-geodyn>

	<struct-sediments>
		<Materials>
			<NbSiliciclastics>3</NbSiliciclastics>
			<NbCarbonates>0</NbCarbonates>
			<NbOrganics>0</NbOrganics>
			<MinSlope>0.0001</MinSlope>
			<si>
				<MaterialName>coarSand</MaterialName>
				<Diameter>0.25</Diameter>
				<Density>2550.0</Density>
				<SlopeMarine>0.017</SlopeMarine>
				<SlopeAerial>0.00265</SlopeAerial>
			</si>
			<si>
				<MaterialName>medSand</MaterialName>
				<Diameter>0.12</Diameter>
				<Density>2550.0</Density>
				<SlopeMarine>0.016</SlopeMarine>
				<SlopeAerial>0.0025</SlopeAerial>
			</si>
			<si>
				<MaterialName>fineSand</MaterialName>
				<Diameter>0.08</Diameter>
				<Density>2550.0</Density>
				<SlopeMarine>0.0015</SlopeMarine>
				<SlopeAerial>0.00235</SlopeAerial>
			</si>
		</Materials>

		<Init_deposit>
			<LayersNb>1</LayersNb>
			<dep>
				<FileName>data/layers.dep</FileName>
			</dep>
		</Init_deposit>
	</struct-sediments>

	<struct-processes>
		<StreamClass>
			<NbClass>3</NbClass>
			<cl>
				<BedSlope>0.0</BedSlope>
				<WDRatio>40</WDRatio>
			</cl>
			<cl>
				<BedSlope>0.02</BedSlope>
				<WDRatio>20</WDRatio>
			</cl>
			<cl>
				<BedSlope>0.2</BedSlope>
				<WDRatio>10</WDRatio>
			</cl>
		</StreamClass>

		<RainfallGrid>
			<rainFlowAccu>60</rainFlowAccu>
			<rainMaxFW>150</rainMaxFW>
			<rainElevation>2.5</rainElevation>
			<rainCombination>0</rainCombination>
			<rainStreamQ>0</rainStreamQ>
			<nbRainfallInterval>1</nbRainfallInterval>
			<rain>
				<startTime>75000</startTime>
				<endTime>4000000</endTime>
				<rainFile>data/rain.rain</rainFile>
			</rain>
		</RainfallGrid>

		<MassMovement>
			<massFlowAccu>20</massFlowAccu>
			<massCurvature>-0.0001</massCurvature>
			<Aerial>
				<upSlopeArea>800.</upSlopeArea>
				<belowUpSlope>
					<steepnessIndex>0.31</steepnessIndex>
					<concavityIndex>0.2</concavityIndex>
				</belowUpSlope>
				<aboveUpSlope>
					<steepnessIndex>0.26</steepnessIndex>
					<concavityIndex>0.</concavityIndex>
				</aboveUpSlope>
			</Aerial>
			<Marine>
				<upSlopeArea>800.</upSlopeArea>
				<belowUpSlope>
					<steepnessIndex>0.31</steepnessIndex>
					<concavityIndex>0.2</concavityIndex>
				</belowUpSlope>
				<aboveUpSlope>
					<steepnessIndex>0.26</steepnessIndex>
					<concavityIndex>0.</concavityIndex>
				</aboveUpSlope>
			</Marine>
		</MassMovement>
	</struct-processes>

	<struct-param>
		<SedimentTransportParameters>
			<streamMinDepth>0.5</streamMinDepth>
			<streamMaxDepth>20</streamMaxDepth>
			<streamMinWidth>1</streamMinWidth>
			<streamMaxWidth>500</streamMaxWidth>
			<flowWalkerMinVelocity>0.0001</flowWalkerMinVelocity>
			<flowWalkerErosionLimit>0.25</flowWalkerErosionLimit>
			<flowWalkerDepositionLimit>0.5</flowWalkerDepositionLimit>
			<minSedimentLoad>0.000000000001</minSedimentLoad>
			<maxFWStepPerCell>180</maxFWStepPerCell>
		</SedimentTransportParameters>

		<Manning>
			<openchannelFlow>0.02</openchannelFlow>
			<hyperpycnalFlow>0.01</hyperpycnalFlow>
			<hypopycnalFlow>0.080</hypopycnalFlow>
		</Manning>
	</struct-param>

	<struct-output>
		<OutputDirectory>outrift</OutputDirectory>

		<OutputWaterLevel>1</OutputWaterLevel>
	</struct-output>

</lecode:lecodeInput>
