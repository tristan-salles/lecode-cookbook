                            </xsd:element-->

                            <xsd:element minOccurs="0" maxOccurs="1" name="Carbonates-Organics">
                                <xsd:annotation>
                                    <xsd:documentation>Define fuzzy rules to calculate carbonates
                                        and organics development.</xsd:documentation>
                                </xsd:annotation>

                                <xsd:complexType>
                                    <xsd:sequence>

                                        <xsd:element name="carborgTime">
                                            <xsd:annotation>
                                                <xsd:documentation> Requested time step used to
                                                  compute carbonates and organics evolution.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  <xsd:attribute name="unit" default="a"
                                                  use="optional"/>
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>

                                        <xsd:element name="tempsalFile" type="xsd:anyURI">
                                            <xsd:annotation>
                                                <xsd:documentation>Path to the temperature and
                                                  salinity definition file, time in years,
                                                  temperature in Celcius and salinity in ppm
                                                  .</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>

                                        <xsd:element name="membershipfunctionNb"
                                            type="xsd:nonNegativeInteger" default="1">
                                            <xsd:annotation>
                                                <xsd:documentation>Total number of membership
                                                  functions</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="fuzzyfunctionNb"
                                            type="xsd:nonNegativeInteger" default="1">
                                            <xsd:annotation>
                                                <xsd:documentation>Total number of fuzzy logic
                                                  functions</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="fuzzyruleNb"
                                            type="xsd:nonNegativeInteger" default="1">
                                            <xsd:annotation>
                                                <xsd:documentation>Total number of fuzzy logic
                                                  rules</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>

                                        <xsd:element minOccurs="1" maxOccurs="unbounded"
                                            name="membershipFunction">
                                            <xsd:annotation>
                                                <xsd:documentation>Parameters definition for the
                                                  considered membership
                                                  function.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="functionName">
                                                  <xsd:annotation>
                                                  <xsd:documentation>User defined membership
                                                  function name. </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:simpleContent>
                                                  <xsd:extension base="xsd:string"> </xsd:extension>
                                                  </xsd:simpleContent>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="functionVariable">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Default variable name to set
                                                  up. </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="depth">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Water depth in metres.
                                                  </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="ocean-energy">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Wind-induced wave and currents
                                                  circulation velocity in the area in m/s. You must
                                                  have waves turn on. </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="shore-distance">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Distance in metres to the
                                                  shoreline. </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="river-distance">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Distance to the nearest river
                                                  source in metres. </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="carbonate-distance">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Distance to nearest surface
                                                  exposed carbonate. </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="organic-distance">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Distance to nearest surface
                                                  exposed organic. </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="sedimentation-rate">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Sediment rate in metres per
                                                  year. </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="temperature">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Surface temperature in Celsius.
                                                  </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="salinity">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Degree of salinity in the
                                                  system in ppm. </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="valley">
                                                  <xsd:annotation>
                                                  <xsd:documentation>The normalise number of
                                                  surrounding points uphill from the reference point
                                                  (calclated from all grid points within two grid
                                                  cells): -1 = all points downhill, 1 = all points
                                                  uphill [-1,1]. </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="gradient">
                                                  <xsd:annotation>
                                                  <xsd:documentation>The average gradient of the
                                                  point (calculated from all grid points within two
                                                  grid cells) negative value indicates that most
                                                  surrounding points are downhill from the
                                                  point.</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="exposed-time">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Time the layer has been
                                                  undisturbed on the surface in years.
                                                  </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="burial">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Depth to which the material has
                                                  been buried by subsequent deposits in metres
                                                  (affects material subsurface).</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="age">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Time since the layer was
                                                  deposited in years (affects material subsurface).
                                                  </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element name="functionPts">
                                                  <xsd:annotation>
                                                  <xsd:documentation> Number of points describing
                                                  the shape of the membership function.
                                                  </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:simpleContent>
                                                  <xsd:extension base="xsd:nonNegativeInteger">
                                                  </xsd:extension>
                                                  </xsd:simpleContent>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="functionSubset">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Parameters definition for the
                                                  considered membership
                                                  function.</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element minOccurs="2" maxOccurs="unbounded"
                                                  name="xypoints">
                                                  <xsd:annotation>
                                                  <xsd:documentation> Function coordinates values. Y
                                                  coordinates must be between 0 and 1.
                                                  </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:list itemType="xsd:decimal"/>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element minOccurs="1" maxOccurs="unbounded"
                                            name="fuzzyFunction">
                                            <xsd:annotation>
                                                <xsd:documentation>Parameters definition for the
                                                  considered fuzzy function.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="fuzzyName">
                                                  <xsd:annotation>
                                                  <xsd:documentation>User defined fuzzy logic
                                                  function name. </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:simpleContent>
                                                  <xsd:extension base="xsd:string"> </xsd:extension>
                                                  </xsd:simpleContent>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="fuzzyVariable">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Default variable name to set up
                                                  fuzzy logic function. </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="growth">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Function controlling carbonates
                                                  or organics growth rate in metres per years.
                                                  </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="erosion">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Function controlling carbonates
                                                  or organics erosion rate in metres per years.
                                                  </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="porosity">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Function controlling carbonates
                                                  or organics porosity between 0 and 1.
                                                  </xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  <xsd:enumeration value="hardness">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Function controlling carbonates
                                                  or organics hardness ( &gt; 1 ).</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:enumeration>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element name="fuzzyPts">
                                                  <xsd:annotation>
                                                  <xsd:documentation> Number of points describing
                                                  the shape of the fuzzy logic function.
                                                  </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:simpleContent>
                                                  <xsd:extension base="xsd:nonNegativeInteger">
                                                  </xsd:extension>
                                                  </xsd:simpleContent>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="fuzzySubset">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Parameters definition for the
                                                  considered fuzzy logic
                                                  function.</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element minOccurs="2" maxOccurs="unbounded"
                                                  name="xycoords">
                                                  <xsd:annotation>
                                                  <xsd:documentation> Fuzzy function coordinates
                                                  values. Y coordinates must be between 0 and 1.
                                                  </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:list itemType="xsd:decimal"/>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element minOccurs="1" maxOccurs="unbounded"
                                            name="fuzzyRule">
                                            <xsd:annotation>
                                                <xsd:documentation>Parameters definition for the
                                                  considered fuzzy rule.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element name="matName">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Name of the carbonate or
                                                  organic type defined in the material element.
                                                  </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:simpleContent>
                                                  <xsd:extension base="xsd:string"> </xsd:extension>
                                                  </xsd:simpleContent>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="fuzzyFctName">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Previously user defined fuzzy
                                                  logic function name to be used with the considered
                                                  material. </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:simpleContent>
                                                  <xsd:extension base="xsd:string"> </xsd:extension>
                                                  </xsd:simpleContent>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="variableNb">
                                                  <xsd:annotation>
                                                  <xsd:documentation> Number of membership functions
                                                  describing the fuzzy rule. </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:simpleContent>
                                                  <xsd:extension base="xsd:nonNegativeInteger">
                                                  </xsd:extension>
                                                  </xsd:simpleContent>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="membershipSet">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Membership function for the
                                                  considered fuzzy rule
                                                  function.</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:sequence>
                                                  <xsd:element minOccurs="1" maxOccurs="unbounded"
                                                  name="membershipFctName">
                                                  <xsd:annotation>
                                                  <xsd:documentation> Previously user defined
                                                  membership functions used to define the fuzzy
                                                  rule.</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:list itemType="xsd:string"/>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  </xsd:sequence>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute ref="xml:base"/>
                                </xsd:complexType>
                            </xsd:element>

                        </xsd:sequence>
                        <xsd:attribute ref="xml:base"/>
                    </xsd:complexType>
                </xsd:element>

                <xsd:element name="struct-param" maxOccurs="1" minOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation> PROCESSES PARAMETERS </xsd:documentation>
                    </xsd:annotation>

                    <xsd:complexType>
                        <xsd:sequence>

                            <xsd:element name="SedimentTransportParameters">
                                <xsd:annotation>
                                    <xsd:documentation> Limiting factors for sediment transport.
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="streamMinDepth">
                                            <xsd:annotation>
                                                <xsd:documentation> Minimum depth for a stream in
                                                  the simulation (m). </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  <xsd:attribute name="unit" default="m"
                                                  use="optional"/>
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="streamMaxDepth">
                                            <xsd:annotation>
                                                <xsd:documentation> Maximum depth for a stream in
                                                  the simulation (m). </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  <xsd:attribute name="unit" default="m"
                                                  use="optional"/>
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="streamMinWidth">
                                            <xsd:annotation>
                                                <xsd:documentation> Minimum width for a stream in
                                                  the simulation (m). </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  <xsd:attribute name="unit" default="m"
                                                  use="optional"/>
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="streamMaxWidth">
                                            <xsd:annotation>
                                                <xsd:documentation> Maximum width for a stream in
                                                  the simulation (m). </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  <xsd:attribute name="unit" default="m"
                                                  use="optional"/>
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="flowWalkerMinVelocity">
                                            <xsd:annotation>
                                                <xsd:documentation> Minimum velocity of flow walkers
                                                  (m/s). When a flow walker velocity decreases below
                                                  this threshold value, all sediment load is
                                                  deposited and the flow walker disappears.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  <xsd:attribute name="unit" default="m/s"
                                                  use="optional"/>
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="flowWalkerErosionLimit">
                                            <xsd:annotation>
                                                <xsd:documentation> Maximum eroded thickness at each
                                                  flow computation step (m) </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  <xsd:attribute name="unit" default="m"
                                                  use="optional"> </xsd:attribute>
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="flowWalkerDepositionLimit">
                                            <xsd:annotation>
                                                <xsd:documentation> Factor controlling maximum
                                                  deposition thickness based on flow
                                                  height.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  <xsd:attribute name="unit" default="m"
                                                  use="optional"> </xsd:attribute>
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="minSedimentLoad">
                                            <xsd:annotation>
                                                <xsd:documentation>Minimum sediment load threshold
                                                  under which the flow walker is erased from the
                                                  simulation based on sediment discharge threshold
                                                  in millions of tons per year. </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  <xsd:attribute name="unit" default="t/a"
                                                  use="optional"/>
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="maxFWStepPerCell" type="xsd:integer">
                                            <xsd:annotation>
                                                <xsd:documentation> Maximum authorised number of
                                                  iteration a flow walker can remain in a given cell
                                                  before forcing it to die. Value ranges between 100
                                                  and 400.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>

                            <xsd:element name="Manning">
                                <xsd:annotation>
                                    <xsd:documentation>Manning's coefficients</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="openchannelFlow" type="xsd:double"
                                            default="0.020">
                                            <xsd:annotation>
                                                <xsd:documentation>Coefficient for open-channel
                                                  flow.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="hyperpycnalFlow" type="xsd:double"
                                            default="0.010">
                                            <xsd:annotation>
                                                <xsd:documentation>Coefficient for hyperpycnal
                                                  flow.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="hypopycnalFlow" type="xsd:double"
                                            default="0.070">
                                            <xsd:annotation>
                                                <xsd:documentation>Coefficient for hypopycnal
                                                  flow.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>

                            <xsd:element maxOccurs="1" minOccurs="0" name="Porosity">
                                <xsd:annotation>
                                    <xsd:documentation>Porosity function definition.
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="EffPressureNb" type="xsd:integer"
                                            minOccurs="1">
                                            <xsd:annotation>
                                                <xsd:documentation> Effective pressure number
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="PorosityFile" type="xsd:anyURI">
                                            <xsd:annotation>
                                                <xsd:documentation> Porosity table file
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>

                        </xsd:sequence>
                        <xsd:attribute ref="xml:base"/>
                    </xsd:complexType>
                </xsd:element>

                <xsd:element name="struct-output" maxOccurs="1" minOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation> OUTPUTS SECTION </xsd:documentation>
                    </xsd:annotation>

                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="OutputDirectory" type="xsd:string" default="outputs">
                                <xsd:annotation>
                                    <xsd:documentation> Name of the output directory. By default a
                                        folder called "outputs" will be created </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>

                            <xsd:element minOccurs="0" maxOccurs="1" name="OutputWaterLevel"
                                default="1">
                                <xsd:annotation>
                                    <xsd:documentation>Sea level output control parameter. 0 means
                                        sea level files do not need to be visualised, otherwise 1
                                        (Default is 1 ). </xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:integer">
                                        <xsd:enumeration value="0">
                                            <xsd:annotation>
                                                <xsd:documentation>No sea level
                                                  output</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:enumeration>
                                        <xsd:enumeration value="1">
                                            <xsd:annotation>
                                                <xsd:documentation>with sea level
                                                  output</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:enumeration>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>

                        </xsd:sequence>
                        <xsd:attribute ref="xml:base"/>
                    </xsd:complexType>
                </xsd:element>

                <xsd:element name="struct-plugin" maxOccurs="1" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation> PLUGIN SECTION </xsd:documentation>
                    </xsd:annotation>

                    <xsd:complexType>
                        <xsd:sequence>

                            <xsd:element minOccurs="0" maxOccurs="1" name="SeismicLine">
                                <xsd:annotation>
                                    <xsd:documentation>Declaration of a 2D seismic line. The line
                                        needs to be in X or Y direction and needs to be aligned with
                                        the mesh gridding</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Xmin">
                                            <xsd:annotation>
                                                <xsd:documentation>X min.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                    <xsd:extension base="xsd:double">
                                                        <xsd:attribute name="unit" default="m"
                                                            use="optional"/>
                                                    </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Xmax">
                                            <xsd:annotation>
                                                <xsd:documentation>X max.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                    <xsd:extension base="xsd:double">
                                                        <xsd:attribute name="unit" default="m"
                                                            use="optional"/>
                                                    </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Ymin">
                                            <xsd:annotation>
                                                <xsd:documentation>Y min.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                    <xsd:extension base="xsd:double">
                                                        <xsd:attribute name="unit" default="m"
                                                            use="optional"/>
                                                    </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Ymax">
                                            <xsd:annotation>
                                                <xsd:documentation>Y max.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                    <xsd:extension base="xsd:double">
                                                        <xsd:attribute name="unit" default="m"
                                                            use="optional"/>
                                                    </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>

                            <xsd:element minOccurs="0" maxOccurs="1" name="RMSModel">
                                <xsd:annotation>
                                    <xsd:documentation>Definition of the geocellular model
                                        extension.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Xmin">
                                            <xsd:annotation>
                                                <xsd:documentation>X min.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                    <xsd:extension base="xsd:double">
                                                        <xsd:attribute name="unit" default="m"
                                                            use="optional"/>
                                                    </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Xmax">
                                            <xsd:annotation>
                                                <xsd:documentation>X max.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                    <xsd:extension base="xsd:double">
                                                        <xsd:attribute name="unit" default="m"
                                                            use="optional"/>
                                                    </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Ymin">
                                            <xsd:annotation>
                                                <xsd:documentation>Y min.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                    <xsd:extension base="xsd:double">
                                                        <xsd:attribute name="unit" default="m"
                                                            use="optional"/>
                                                    </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Ymax">
                                            <xsd:annotation>
                                                <xsd:documentation>Y max.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                    <xsd:extension base="xsd:double">
                                                        <xsd:attribute name="unit" default="m"
                                                            use="optional"/>
                                                    </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>

                            <xsd:element minOccurs="0" maxOccurs="1" name="UnderworldPlugin">
                                <xsd:annotation>
                                    <xsd:documentation>Definition of the Underworld / LECODE plugin
                                        simulation.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="UsyncFolder" type="xsd:anyURI">
                                            <xsd:annotation>
                                                <xsd:documentation>Path to the synchronisation
                                                  folder.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="UsyncTime">
                                            <xsd:annotation>
                                                <xsd:documentation>Time for synchronisation of
                                                  LECODE and Underworld.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  <xsd:attribute name="unit" default="a"
                                                  use="optional"/>
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>

                            <xsd:element maxOccurs="1" minOccurs="0" name="OceanPlugin">
                                <xsd:annotation>
                                    <xsd:documentation>Ocean plugin for waves and currents
                                        circulation.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="circForcing">
                                            <xsd:annotation>
                                                <xsd:documentation>Ocean circulation is enable if 1
                                                  otherwise 0.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:integer">
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="waveForcing">
                                            <xsd:annotation>
                                                <xsd:documentation>Wave induced currents is enable
                                                  if 1 otherwise 0.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:integer">
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="OsyncFolder" type="xsd:string">
                                            <xsd:annotation>
                                                <xsd:documentation>Give the synchronization folder
                                                  name.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="OsyncTime">
                                            <xsd:annotation>
                                                <xsd:documentation> Synchronization time in years.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  <xsd:attribute name="unit" default="a"
                                                  use="optional"/>
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="morphLimit">
                                            <xsd:annotation>
                                                <xsd:documentation> Morphological change thickness
                                                  limiter. </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  <xsd:attribute name="unit" default="m"
                                                  use="optional"/>
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="morphAc">
                                            <xsd:annotation>
                                                <xsd:documentation> Morphological acceleration
                                                    factor. Long-term morphological evolution is
                                                    achieved using hydrodynamic calculations of only a
                                                    fraction of the simulated duration. This need to
                                                    be calibrated based on temporal and spatial
                                                    resolution of the considered simulation. This
                                                    parameter is used in relation to the Ocean plugin
                                                    only. </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                    <xsd:extension base="xsd:double"> </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="nbForecast">
                                            <xsd:annotation>
                                                <xsd:documentation> Number of wave climate records
                                                  processed during the simulation time. For instance
                                                  if different statistical wave/wind regimes exist
                                                  between summer and winter seasons the user has the
                                                  possibility to define several seasonal wave/wind
                                                  cycle that occur during the considered time. The
                                                  number of forecast regimes is constant during the
                                                  simulation which means that the user needs to
                                                  define for each wave/wind regimes interval the
                                                  same number of forecasts scenarios.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                  <xsd:extension base="xsd:integer">
                                                  </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element maxOccurs="unbounded" minOccurs="1"
                                            name="forecastClass">
                                            <xsd:annotation>
                                                <xsd:documentation> Declaration of wind hindcast
                                                  regime for each forecast subgroup. The number of
                                                  classes needs to be equal to the number given for
                                                  the 'nbForecast' variable.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                  <xsd:element minOccurs="1" maxOccurs="1"
                                                  name="subgroupNb">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Number of subgroup forces
                                                  defined for the considered forecast class.
                                                  </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:list itemType="xsd:integer"/>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:element minOccurs="1" maxOccurs="1"
                                                  name="Ostart">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Starting time of the considered
                                                  forecast in year. </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  <xsd:attribute name="unit" default="a"/>
                                                  </xsd:extension>
                                                  </xsd:simpleContent>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element minOccurs="1" maxOccurs="1"
                                                  name="Oend">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Ending time of the considered
                                                  forecast in year. </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  <xsd:attribute name="unit" default="a"/>
                                                  </xsd:extension>
                                                  </xsd:simpleContent>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element minOccurs="1" maxOccurs="unbounded"
                                                  name="subgroupProp">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Proportion of considered wind
                                                  subgroup active over a year.
                                                  </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:complexType>
                                                  <xsd:simpleContent>
                                                  <xsd:extension base="xsd:double">
                                                  </xsd:extension>
                                                  </xsd:simpleContent>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>

                        </xsd:sequence>
                        <xsd:attribute ref="xml:base"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
